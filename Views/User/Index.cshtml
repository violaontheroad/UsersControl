@model IEnumerable<User>
@{
    ViewData["Title"] = "Index";
}

<h1>Index User</h1>

<table class="table table-bordered table-striped" style="width:100%">
  <thead>
    <tr>
      <th>
        Name
      </th>
      <th>
        Email
        </th>
      <th>
        CPF
        </th>
    </tr>
  </thead>
  <tbody>
      @foreach (var obj in Model)
      {
        <tr>
            <td width="50%">
              @obj.Name
            </td>
             <td width="30%">
              @obj.Cpf
            </td>
        </tr>
      }
  </tbody>
</table>

</div>

